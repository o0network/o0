{
  "name": "app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start:mobile": "expo start --config app.mobile.json",
    "start:web": "expo start --web --config app.web.json",
    "start:tg": "expo start --web --config app.tg.json",
    "build:web": "expo export -p web --config app.web.json",
    "build:tg": "expo export -p web --config app.tg.json",
    "build:mobile": "eas build --platform all --profile production",
    "deploy:web": "expo export -p web --config app.web.json",
    "deploy:tg": "expo export -p web --config app.tg.json",
    "deploy:mobile": "eas build --platform all --profile production"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-community/slider": "^4.5.6",
    "@twa-dev/sdk": "^6.9.2",
    "@types/three": "^0.175.0",
    "expo": "~52.0.43",
    "expo-blur": "^14.0.3",
    "expo-font": "^13.0.4",
    "expo-linear-gradient": "^14.0.2",
    "expo-router": "^4.0.20",
    "expo-sensors": "~14.0.2",
    "expo-status-bar": "~2.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-reanimated": "^3.17.3",
    "react-native-safe-area-context": "^4.11.1",
    "react-native-svg": "^15.11.2",
    "react-native-web": "~0.19.9",
    "react-native-webview": "^13.2.2",
    "three": "^0.175.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "eas-cli": "^5.9.1",
    "typescript": "^5.3.3"
  },
